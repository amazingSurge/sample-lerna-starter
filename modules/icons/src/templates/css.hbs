@font-face {
  font-family: "{{fontName}}";
  src: {{{src}}};
}

{{baseSelector}} {
  line-height: 1;
}

{{baseSelector}}:before {
  font-family: {{fontName}} !important;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  text-decoration: none;
  text-transform: none;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
}
{{/each}}